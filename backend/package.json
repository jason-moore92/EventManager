{
  "name": "tva-services",
  "version": "1.1.0",
  "description": "Node.js express.js MongoDB JWT REST API - This is a basic API REST skeleton written on JavaScript using async/await. Great for building a starter web API for your front-end (Android, iOS, Vue, react, angular, or anything that can consume an API)",
  "license": "MIT",
  "scripts": {
    "start": "nodemon NODE_ENV=development server.js",
    "start1": "cross-env NODE_ENV=production pm2-runtime start server.js",
    "mocha": "nyc mocha --timeout=5000 --exit",
    "test": "npm run fresh && npm run mocha",
    "utest": "mocha unit_tests --file unit_tests/setup.js --timeout=10000 --exit",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "fresh": "npm run clean && npm run seed",
    "clean": "node clean.js",
    "seed": "node seed.js",
    "format": "prettier --write --config .prettierrc.json \"**/*.js\"",
    "lint": "eslint --fix --config .eslintrc.json \"**/*.js\"",
    "remark": "remark . -o",
    "deploy:qa": "aws --profile MakeOwnStore codepipeline start-pipeline-execution --name tmsahaya-qa",
    "deploy:prod": "aws --profile MakeOwnStore codepipeline start-pipeline-execution --name tmsahaya-prod",
    "invoke:qa": "aws lambda invoke --function-name tmsahaya-qa --invocation-type Event --payload tmsahaya_qa_invoke.json",
    "invoke:prod": "aws lambda invoke --function-name tmsahaya-prod --invocation-type Event --payload tmsahaya_prod_invoke.json"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.332.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.4.0",
    "firebase-admin": "^9.4.2",
    "helmet": "^3.21.3",
    "i18n": "^0.8.6",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.6",
    "mongoose-aggregate-paginate-v2": "^1.0.42",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.1",
    "passport.socketio": "^3.7.0",
    "sanitizer": "^0.1.3",
    "trim-request": "^1.0.6",
    "uuid": "^7.0.2",
    "validator": "^12.2.0",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2"
  },
  "keywords": [
    "javascript",
    "api",
    "node",
    "express",
    "mongo",
    "mongodb",
    "jwt",
    "postman",
    "i18n",
    "jwt-authentication",
    "token",
    "eslint",
    "starter",
    "web",
    "app",
    "mongoose",
    "rest",
    "skeleton",
    "async",
    "await",
    "mvp",
    "front-end",
    "testing",
    "prettier",
    "chai",
    "JSDoc"
  ]
}
